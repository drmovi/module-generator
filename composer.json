{
    "name": "drmovi/laravel-module",
    "description": "Laravel package for generating modular applications",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "autoload": {
        "psr-4": {
            "Drmovi\\LaravelModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drmovi\\LaravelModule\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Drmovi",
            "email": "info@drmovi.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0|^10.0",
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drmovi\\LaravelModule\\LaravelModuleServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
